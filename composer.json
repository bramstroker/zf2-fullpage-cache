{
    "name": "stroker/cache",
    "description": "Provides a full page cache solution for ZF2",
    "type": "library",
    "keywords": [
        "zf2",
        "cache",
        "fpc"
    ],
    "homepage": "https://github.com/bramstroker/zf2-fullpage-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Gerritsen",
            "email": "bgerritsen@gmail.com"
        },
        {
            "name": "Freek Gruntjes"
        }
    ],
    "require": {
        "php": ">=5.6",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-eventmanager": "^2.6.3 || ^3.1",
        "zendframework/zend-servicemanager": "^2.7.8 || ^3.2",
        "zendframework/zend-http": "^2.6",
        "zendframework/zend-mvc": "^2.7 || ^3.0",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-console": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5",
        "mockery/mockery": "*",
        "mikey179/vfsStream": "~1.6"
    },
    "autoload": {
        "psr-4": {
            "StrokerCache\\": "src/",
            "StrokerCacheTest\\": "test/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
